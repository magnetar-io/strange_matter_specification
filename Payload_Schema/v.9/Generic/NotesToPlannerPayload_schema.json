{
    "$schema": "https://github.com/magnetar-io/strange_matter_specification/blob/main/Payload_Schema/v.9/NotesToPlannerPayload_schema.json",
    "title": "NotesToPlanner",
    "type": "object",
    "properties": {
      "KnowledgeCollected": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "location": {
              "type": "string"
            },
            "details": {
              "type": "string"
            },
            "URI": {
              "type": "string",
              "format": "uri"
            }
          },
          "required": ["location", "details", "URI"]
        }
      },
      "AISummary": {
        "type": "object",
        "properties": {
          "summary": {
            "type": "string"
          },
          "URI": {
            "type": "string",
            "format": "uri"
          }
        },
        "required": ["summary", "URI"]
      }
    },
    "required": ["KnowledgeCollected", "AISummary"]
  }
  