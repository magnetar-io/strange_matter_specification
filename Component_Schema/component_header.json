{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "StrangeMatter Header Component Schema",
    "source": ""
    "description": "A schema for representing component header information",
    "version": "0.0.1",
    "hash": {"type": "string"},
    "dateCreated": "2023-12-19",
    "type": "object",
    "properties": {
      "Component_Header_Source": {
        "type": "object",
        "properties": {
          "component_type": {"type": "string"},
          "component_type_reference": {"type": "string", "format": "uri"},
          "component_type_guid": {"type": "string", "format": "uuid"},
          "component_type_version_guid": {"type": "string", "format": "uuid"},
          "component_version": {"type": "string"},
          "component_hash": {"type": "string"}
        },
        "required": ["component_type", "component_type_reference", "component_type_guid", "component_type_version_guid", "component_version", "component_hash"]
      },
      "Component_Header_Data_Source": {
        "type": "object",
        "properties": {
          "author": {"type": "string"},
          "author_identifier": {"type": "string"},
          "context": {"type": "string"},
          "source_data": {"type": "string", "format": "uri"},
          "source_data_file_date": {"type": "string", "format": "date"},
          "source_data_file_id": {"type": "string"},
          "source_data_file_version_id": {"type": "string"},
          "source_data_item_id": {"type": "string"},
          "source_data_item_version_id": {"type": "string"},
          "source_data_other": {"type": "string"}
        },
        "required": ["author", "author_identifier", "context", "source_data", "source_data_file_date", "source_data_file_id", "source_data_file_version_id", "source_data_item_id", "source_data_item_version_id"]
      },
      "Component_Header_Strange_Matter_Identifiers": {
        "type": "object",
        "properties": {
          "entity_guid": {"type": "string", "format": "uuid"},
          "component_guid": {"type": "string", "format": "uuid"},
          "version": {"type": "string"},
          "version_guid": {"type": "string", "format": "uuid"},
          "component_hash": {"type": "string"},
          "version_increment": {"type": "string"},
          "date_created": {"type": "string", "format": "date-time"},
          "name": {"type": "string"},
          "classification_value": {"type": "string"},
          "sequence_name": {"type": "string"},
          "sequence_guid": {"type": "string", "format": "uuid"},
          "sequence_value": {"type": "string"},
          "option": {"type": "string"},
          "phase": {"type": "string"},
          "status": {"type": "string"},
          "active": {"type": "string"}
        },
        "required": ["entity_guid", "component_guid", "version", "version_guid", "component_hash", "version_increment", "date_created", "name", "classification_value", "status", "active"]
      },
      "Component_Header_Responses": {
        "type": "object",
        "properties": {
          "component_responses": {
            "type": "array",
            "items": {"type": "string"}
          }
        }
      },
      "Component_Header_Payload_Details": {
        "type": "object",
        "properties": {
          "payload_hash": {"type": "string"},
          "hash_definition": {"type": "string"},
          "payload_data_type": {"type": "string"},
          "payload_data_type_definition": {"type": "string"},
          "payload_encoding": {"type": "string"},
          "payload_encryption": {"type": "string"}
        },
        "required": ["payload_hash", "hash_definition", "payload_data_type", "payload_data_type_definition", "payload_encoding", "payload_encryption"]
      }
    }
  }
  